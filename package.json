{
  "name": "wrestly",
  "version": "0.1.0",
  "description": "Wrestle that rest",
  "author": "Darren Royle @royletron",
  "bin": {
    "wrestly": "./bin/run"
  },
  "homepage": "https://github.com/ventureharbour/wrestly",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "ventureharbour/wrestly",
  "files": [
    "/bin",
    "/dist",
    "/.next",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "1.3.4",
    "@vscode/sqlite3": "5.0.7",
    "express": "4.17.3",
    "knex": "1.0.3",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.1.2",
    "timeago-react": "3.0.4"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "@types/react": "^17.0.39",
    "eslint-config-next": "12.2.0"
  },
  "oclif": {
    "bin": "wrestly",
    "dirname": "wrestly",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {
      "run": {
        "description": "Get started with rest"
      }
    }
  },
  "scripts": {
    "dev": "next dev",
    "build:next": "next build",
    "build": "shx rm -rf dist && shx rm -rf .next && next build && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ventureharbour/wrestly/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
